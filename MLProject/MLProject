name: Churn_Prediction_Pipeline

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      data_file: {type: string, default: "online_retail_customer_churn_preprocessing.csv"}
      n_estimators: {type: int, default: 100}
    command: "python modelling.py --data_file {data_file} --n_estimators {n_estimators}"